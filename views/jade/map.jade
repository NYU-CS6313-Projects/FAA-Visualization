doctype html
html(lang='en')
  head
    title= title
    link(rel='shortcut icon' href='/images/airplane.png')
    link(rel='stylesheet', href='/stylesheets/reset.css')
    link(rel='stylesheet', href='https://cdnjs.cloudflare.com/ajax/libs/materialize/0.95.3/css/materialize.min.css')
    link(rel='stylesheet', href='/stylesheets/style.css')
    link(rel='stylesheet', href='/stylesheets/map.css')
    script(type='type/javascript', src='//cdnjs.cloudflare.com/ajax/libs/lodash.js/3.5.0/lodash.min.js')
    script(type='text/javascript', src='//cdnjs.cloudflare.com/ajax/libs/d3/3.5.5/d3.min.js')
    script(type='text/javascript', src='//cdnjs.cloudflare.com/ajax/libs/topojson/1.6.9/topojson.min.js')
    script(type='text/javascript', src='//cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js')
  body
    h1= title
    p Welcome to #{title}
    div
      a.waves-effect.waves-light.btn
        | Stuff
      form(action='#')
        p
          input(name='group1', type='radio', id='test1')
          label(for='test1')
            | Cruising
        p
          input(name='group1', type='radio', id='test2')
          label(for='test2')
            | Taxiing
        p
          input.with-gap(name='group1', type='radio', id='test3')
          label(for='test3')
            | In-Flight
        p
          input(name='group1', type='radio', id='test4', disabled='disabled')
          label(for='test4')
            | Passengers
    script(type='text/javascript', src='/javascripts/minified/map.min.js')